<?xml version="1.0" encoding="utf-8"?>
<PreferenceScreen xmlns:android="http://schemas.android.com/apk/res/android">

    <ListPreference
        android:key="pref_gapInterval"
        android:title="@string/pref_gapInterval"
        android:dialogTitle="@string/pref_gapInterval"
        android:entries="@array/pref_gapInterval_entries"
        android:entryValues="@array/pref_gapInterval_values"
        android:defaultValue="@string/pref_gapInterval_default" />

    <CheckBoxPreference
        android:key="pref_gapSound"
        android:title="@string/pref_gapSound"
        android:defaultValue="true" />

    <CheckBoxPreference
        android:key="pref_name"
        android:title="@string/pref_name"
        android:defaultValue="true" />

</PreferenceScreen>
